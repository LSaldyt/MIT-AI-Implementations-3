#!/usr/bin/env python3
from lib import InstanceMap

from pprint import pprint

def main():
    cmap = InstanceMap('data/test.json')
    pprint(cmap.instances)
    #print(cmap)
    #print(cmap.relations('speedometer', 'odometer'))
    #print(cmap.level_key_values('speedometer'))
    #print(cmap.level_key_values('odometer'))

    frame = cmap.create_frame('speedometer', 'odometer')
    frame.show()

    cmap = InstanceMap('data/blockworld.json')
    pprint(cmap.instances)
    cmap.update_map()
    pprint(cmap.map)

if __name__ == '__main__':
    main()
